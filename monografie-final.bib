	issn = {02722712},
	journal = {Clinics in Laboratory Medicine},
	keywords = {Coronaviruses,Human coronavirus 229E,Human coronavirus HKU1,Human coronavirus NL63,Human coronavirus OC43,Severe acute respiratory syndrome-associated coron},
	number = {4},
