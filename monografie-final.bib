	number = {2},
	pages = {1--18},
	pmid = {33596266},
	title = {{SARS-CoV-2 tropism, entry, replication, and propagation: Considerations for drug discovery and development}},
	url = {http://dx.doi.org/10.1371/journal.ppat.1009225},
	volume = {17},
	year = {2021},
	Bdsk-Url-1 = {http://dx.doi.org/10.1371/journal.ppat.1009225},
