	issn = {1680-743X},
	journal = {Journal of data science : JDS},
	keywords = {analysis,basic reproduction number,epidemic doubling time,incubation period,meta-analysis,sensitivity,serial interval},
	number = {3},
	pages = {536--549},
	pmid = {33088292},
