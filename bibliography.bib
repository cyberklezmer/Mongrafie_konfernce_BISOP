	issn = {1474-547X (Electronic)},
	journal = {Lancet (London, England)},
	keywords = {Betacoronavirus,Bronchoalveolar Lavage Fluid,COVID-19,China,Coronavirus Infections,DNA,Disease Reservoirs,Genome,Genomics,High-Throughput Nucleotide Sequencing,Humans,Phylogeny,Pneumonia,Receptors,SARS-CoV-2,Sequence Alignment,Viral,Virus,diagnosis,epidemiology,genetics,metabolism,methods,transmission,virology},
	language = {eng},
