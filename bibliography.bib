	doi = {10.1038/s41591-020-0968-3},
	file = {:Users/jantrnka/Mendeley Desktop/Gupta et al. - 2020 - Extrapulmonary manifestations of COVID-19.pdf:pdf},
	issn = {1546170X},
	journal = {Nature Medicine},
	number = {7},
