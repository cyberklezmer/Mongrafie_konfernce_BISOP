	issn = {1476-4687},
	journal = {Nature},
	pmid = {33137809},
	title = {{Age-specific mortality and immunity patterns of SARS-CoV-2.}},
	url = {http://www.ncbi.nlm.nih.gov/pubmed/33137809},
	year = {2020},
	Bdsk-Url-1 = {http://www.ncbi.nlm.nih.gov/pubmed/33137809},
	Bdsk-Url-2 = {https://doi.org/10.1038/s41586-020-2918-0}}
	
	
